{
	"name": "constants",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "src/constants/src",
	"prefix": "lib",
	"tags": [],
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/angular:ng-packagr-lite",
			"outputs": ["{workspaceRoot}/dist/{projectRoot}"],
			"options": {
				"project": "src/constants/ng-package.json"
			},
			"configurations": {
				"production": {
					"tsConfig": "src/constants/tsconfig.lib.prod.json"
				},
				"development": {
					"tsConfig": "src/constants/tsconfig.lib.json"
				}
			},
			"defaultConfiguration": "production"
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "src/constants/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["src/constants/**/*.ts", "src/constants/**/*.html", "src/constants/package.json"]
			}
		}
	}
}
