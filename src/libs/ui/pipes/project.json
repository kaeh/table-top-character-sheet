{
  "name": "pipes",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src/libs/ui/pipes/src",
  "prefix": "lib",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "src/libs/ui/pipes/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "src/libs/ui/pipes/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "src/libs/ui/pipes/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "src/libs/ui/pipes/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "src/libs/ui/pipes/**/*.ts",
          "src/libs/ui/pipes/**/*.html",
          "src/libs/ui/pipes/package.json"
        ]
      }
    }
  }
}
